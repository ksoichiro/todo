{
  "name": "todo",
  "description": "bower components for todo UI.",
  "dependencies": {
    "jquery": "2.1.3",
    "bootstrap": "3.3.4"
  },
  "overrides": {
    "jquery": {
      "ignore": ["*.json"]
    },
    "bootstrap": {
      "ignore": [
